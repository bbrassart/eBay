<div class="container">
	<div class="row">
		<div class="col-md-6">
			<%= form_for user, :html => {:multipart => true }  do |f| %>
			<%= f.label :logo %>
			<%= f.file_field :logo %>

			<%= render 'layouts/errors', model: @user %>
		</div>
		<div class="col-md-6">
			<div class="field">
				<%= f.label :name %>
				<%=f.text_field :name %>
			</div>
			<div class="field">
				<%= f.label :email %>
				<%=f.text_field :email %>
			</div>

			<div class="actions">
			<%= f.submit %>
			</div>
			<% end %>
		</div>
	</div>
</div>